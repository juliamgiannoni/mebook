<%= form_for @user, url: users_path, html: { multipart: true } do |form| %>
    <%= form.label :first_name %>
    <%= form.text_field :first_name %><br>

    <%= form.label :last_name %>
    <%= form.text_field :last_name %><br>

    <%= form.file_field :image %><br><br>

    <%= form.submit 'Done' %>

<% end %>
